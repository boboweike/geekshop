query GetCustomerHistory($id: ID!, $options: HistoryEntryListOptions) {
    customer(id: $id) {
        id
        history(options: $options) {
            totalItems
            items {
                id
                administrator {
                    id
                }
                type
                data
            }
        }
    }
}